stage('SonarQube analysis') {
        steps {
            script{
            def scannerHome = tool 'sonarscan';
            withSonarQubeEnv('sonarqube') {
                sh "${tool("sonarscan")}/bin/sonar-scanner \
                    -Dsonar.projectKey=Jenkin-Test-Project \
                    -Dsonar.projectName=Jenkin-Test-Project"
            }
            }
        }
    }
