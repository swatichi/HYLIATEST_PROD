pipeline {
    agent any
    stages {
     stage('Clone sources') {
            steps {
                git url: 'https://github.com/swatichi/HYLIATEST_PROD/'
            }
        }
           stage('SonarQube analysis') {
            steps {
                withSonarQubeEnv('SonarQube') {
                    sh "SonarScanner.MSBuild.exe"
                }
            }
        }
           }
}
